# Server Settings
PORT=8000
WORKERS=4
LOG_LEVEL=info
RELOAD=false

# Cache Settings
FASTF1_CACHE_DIR=/app/fastf1_cache
CACHE_TTL=3600

# Security Settings
ALLOWED_ORIGINS=["https://yourdomain.com"]
API_KEY=  # Set this to a secure random string
RATE_LIMIT=5000  # Requests per minute

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090